{
  "version": 1,
  "framework": { "name": "express", "version": "5.1.0" },
  "routes": [
    {
      "path": "/api/*",
      "target": {
        "kind": "Compute",
        "src": "default"
      }
    },
    {
      "path": "/_health",
      "target": {
        "kind": "Compute",
        "src": "default"
      }
    },
    {
      "path": "/*",
      "target": {
        "kind": "Static"
      },
      "fallback": {
        "rewrite": "/index.html"
      }
    }
  ],
  "computeResources": [
    {
      "name": "default",
      "runtime": "nodejs20.x",
      "entrypoint": "src/index.js",
      "healthCheck": {
        "path": "/_health",
        "initialDelaySeconds": 10,
        "periodSeconds": 5,
        "timeoutSeconds": 5,
        "successThreshold": 1,
        "failureThreshold": 3
      }
    }
  ]
} 