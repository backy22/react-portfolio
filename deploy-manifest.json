{
  "version": 1,
  "appRoot": ".",
  "routes": [
    {
      "path": "/api/*",
      "target": "/compute/default"
    },
    {
      "path": "/_health",
      "target": "/compute/default"
    },
    {
      "path": "/*",
      "target": "/static",
      "fallback": "/static/index.html"
    }
  ],
  "compute": {
    "default": {
      "runtime": "nodejs20.x",
      "handler": "src/index.js",
      "memory": 512,
      "environment": {
        "NODE_ENV": "production"
      }
    }
  }
} 