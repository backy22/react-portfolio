{
  "version": 1,
  "routes": [
    {
      "source": "/api/*",
      "target": "compute/default"
    },
    {
      "source": "/_health",
      "target": "compute/default"
    },
    {
      "source": "/*",
      "target": "static",
      "rewrite": "index.html"
    }
  ],
  "artifacts": {
    "compute": {
      "default": {
        "handler": "index.js",
        "runtime": "nodejs20.x",
        "memory": 512,
        "environment": {
          "NODE_ENV": "production"
        }
      }
    },
    "static": {
      "baseDirectory": "static"
    }
  }
} 